package net.tc.stresstool.statistics;
/*
 * Element containing the information for each event
 * the event is the NAME of the event
 * while the value has to possible field absolute and relative
 * absolute is the reported as it is
 * relative or DELTA is the calculation of the difference from the previous reading 
 * this is valid ONLY in case of numeric object
 */


/**
 * @author  tusa
 */
public class StatEvent {
    /**
     * @uml.property  name="id"
     */
    long id = 0 ;
    /**
     * @uml.property  name="time"
     */
    long time = 0;
    /**
     * @uml.property  name="order"
     */
    int order = 0;
    /**
     * @uml.property  name="collection"
     */
    String collection;
    /**
     * @uml.property  name="parent"
     */
    String parent;
    /**
     * @uml.property  name="event"
     */
    String event ;
    /**
     * @uml.property  name="absoluteValue"
     */
    Object absoluteValue ;
    /**
     * @uml.property  name="deltaValue"
     */
    Object deltaValue; 
    /**
     * @return    the deltaValue
     * @uml.property  name="deltaValue"
     */
    public final Object getDeltaValue() {
        return deltaValue;
    }

    /**
     * @param deltaValue    the deltaValue to set
     * @uml.property  name="deltaValue"
     */
    public final void setDeltaValue(Object deltaValue) {
        this.deltaValue = deltaValue;
    }

    /**
     * @uml.property  name="provider"
     */
    String provider = null;
    
    /**
     * @return    the id
     * @uml.property  name="id"
     */
    public final long getId() {
        return id;
    }

    /**
     * @param id    the id to set
     * @uml.property  name="id"
     */
    public final void setId(long id) {
        this.id = id;
    }

    /**
     * @return    the time
     * @uml.property  name="time"
     */
    public final long getTime() {
        return time;
    }

    /**
     * @param time    the time to set
     * @uml.property  name="time"
     */
    public final void setTime(long time) {
        this.time = time;
    }

    /**
     * @return    the order
     * @uml.property  name="order"
     */
    public final int getOrder() {
        return order;
    }

    /**
     * @param order    the order to set
     * @uml.property  name="order"
     */
    public final void setOrder(int order) {
        this.order = order;
    }

    /**
     * @return    the collection
     * @uml.property  name="collection"
     */
    public final String getCollection() {
        return collection;
    }

    /**
     * @param collection    the collection to set
     * @uml.property  name="collection"
     */
    public final void setCollection(String collection) {
        this.collection = collection;
    }

    /**
     * @return    the parent
     * @uml.property  name="parent"
     */
    public final String getParent() {
        return parent;
    }

    /**
     * @param parent    the parent to set
     * @uml.property  name="parent"
     */
    public final void setParent(String parent) {
        this.parent = parent;
    }

    /**
     * @return    the event
     * @uml.property  name="event"
     */
    public final String getEvent() {
        return event;
    }

    /**
     * @param event    the event to set
     * @uml.property  name="event"
     */
    public final void setEvent(String event) {
        this.event = event;
    }

    /**
     * @return the value
     */
    public final Object getValue() {
        return absoluteValue;
    }

    /**
     * @param value the value to set
     */
    public final void setValue(Object value) {
        this.absoluteValue = value;
    }

    /**
     * @return    the provider
     * @uml.property  name="provider"
     */
    public final String getProvider() {
        return provider;
    }

    /**
     * @param provider    the provider to set
     * @uml.property  name="provider"
     */
    public final void setProvider(String provider) {
        this.provider = provider;
    }

    public StatEvent() {
	// TODO Auto-generated constructor stub
    }

}
